<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.JJJTeam.Jmarket.Mapper.UserMapper">
	<insert id="addUser" parameterType="com.JJJTeam.Jmarket.DTO.UserDTO">
		insert into test (id, pw) values(#{id}, #{pw})
	</insert>
	<select id="selectUser" parameterType="com.JJJTeam.Jmarket.DTO.UserDTO" resultType="com.JJJTeam.Jmarket.DTO.UserDTO">
		select * from test where id = #{loginID} and pw = #{loginPW}
	</select>
</mapper>